len=${#dati[@]}
while :
do

	i=$((RANDOM % len))
	echo "${dati[$i]}"
	#sed -i -e "s/content = .*/content = ${dati[$i]}/g" $HOME/.config/polybar/polybar-6/mymodul.ini
